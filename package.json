{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "devDependencies": {
    "@redwoodjs/cli-storybook": "7.0.0-canary.227",
    "@redwoodjs/core": "7.0.0-canary.227",
    "prettier-plugin-tailwindcss": "^0.4.1"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "=18.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "deploy": "flyctl deploy --wait-timeout 120",
    "deploy:clean": "flyctl deploy --no-cache --wait-timeout 120",
    "deploy:status": "flyctl status"
  },
  "packageManager": "yarn@3.6.1"
}
